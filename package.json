    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "eslint-plugin-import": "^2.28.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
