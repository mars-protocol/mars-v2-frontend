    "@splinetool/react-spline": "^2.2.6",
    "@splinetool/runtime": "^0.9.477",
    "@tanstack/react-table": "^8.10.6",
    "@tippyjs/react": "^4.2.6",
    "bignumber.js": "^9.1.2",
    "classnames": "^2.3.2",
    "debounce-promise": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "next": "^13.5.4",
